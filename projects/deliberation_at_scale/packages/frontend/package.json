{
  "name": "@deliberation-at-scale/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install",
    "setup:env": "cp .env.example .env",
    "start": "next dev -p 3200",
    "build": "next build",
    "link": "vercel link",
    "deploy": "npm run lingui:extract && vercel --prod",
    "start:prod": "next start -p 3300",
    "start:proxy": "npx local-ssl-proxy --source 3201 --target 3400",
    "db:generate-types": "dotenv -e .env -- sh -c 'supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > ./types/database.ts' && eslint --fix ./types/database.ts",
    "db:login": "supabase login",
    "codegen": "dotenv -e .env -- gql-gen --config codegen.ts",
    "codegen:watch": "npm run codegen -- --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf ./node_modules",
    "lingui:extract": "lingui extract --clean && lingui compile",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "author": "Pepijn Verburg <pepijn@bmd.studio>, Lei Nelissen <lei@bmd.studio>, Jorim Theuns <jorim@dembrane.com>",
  "license": "MIT",
  "dependencies": {
